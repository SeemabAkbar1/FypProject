@{
    Layout = "~/Views/Shared/_HeaderLayout.cshtml";
}
@section Header
{
    <script type="text/javascript" src="@Url.Content("~/Scripts/mywebapp/ApplicationView/mywebapp.ui.header.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/mywebapp/ChangePassword/mywebapp.ui.ResetPassword1.js")"></script>

}
@section scripts
{
    <script type="text/javascript">

        try { ace.settings.loadState('main-container') } catch (e) { }

        $(document).ready(function () {
            MyWebApp.UI.ResetPassword1.readyMain();
        });
    </script>
}
<!-- ace settings handler -->
<script src="~/Scripts/ace-extra.min.js"></script>
<input type="hidden" id="txtData" value="@ViewBag.data" />
<div class="main-container ace-save-state" id="main-container">
    <div class="main-content">
        <div class="main-content-inner">
            <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                <ul class="breadcrumb">
                    <a href="@Url.Action("Inbox","Home")" class="btn-back-message-list">
                        <i class="ace-icon fa fa-arrow-left blue bigger-110 middle"></i>
                        <b class="bigger-110 middle">Back</b>
                    </a>
                    <li class="active" id="ApplicationId" style="display:none">@ViewBag.id</li>
                </ul><!-- /.breadcrumb -->

            </div>
            <div class="page-content">

                <div class="page-header">
                    <h1>
                        Account
                        <small>
                            <i class="ace-icon fa fa-angle-double-right"></i>
                            Change Password
                        </small>
                    </h1>
                </div><!-- /.page-header -->
                <div class="row">
                    <div class="col-xs-10">
                        <!-- PAGE CONTENT BEGINS -->

                        <div id="user-profile-3" class="user-profile row">
                            <div class="col-md-offset-3 col-sm-8">
                                <form class="form-horizontal">
                                    <div class="tabbable">
                                        <ul class="nav nav-tabs padding-16">

                                            <li class="active">
                                                <a data-toggle="tab" href="#edit-password">
                                                    <i class="blue ace-icon fa fa-key bigger-125"></i>
                                                    Password
                                                </a>
                                            </li>
                                        </ul>
                                        <div class="tab-content profile-edit-tab-content">

                                            <div id="edit-password">
                                                <div class="space-10"></div>

                                                <div class="space-4"></div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label no-padding-right" for="form-field-pass2">New Password</label>
                                                    <div class="col-sm-9">
                                                        <input type="password" id="form-field-pass2" />
                                                    </div>
                                                </div>
                                                <div class="space-4"></div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label no-padding-right" for="form-field-pass3">Confirm New Password</label>
                                                    <div class="col-sm-9">
                                                        <input type="password" id="form-field-pass3" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="clearfix form-actions">
                                        <div class="col-md-offset-3 col-md-9">
                                            <button class="btn btn-info" type="button" id="SaveButton">
                                                <i class="ace-icon fa fa-check bigger-110"></i>
                                                Save
                                            </button>
                                            &nbsp; &nbsp;
                                            <button class="btn" type="reset">
                                                <i class="ace-icon fa fa-undo bigger-110"></i>
                                                Reset
                                            </button>
                                        </div>
                                    </div>
                                </form>
                            </div><!-- /.span -->
                        </div><!-- /.user-profile -->
                    </div>
                </div><!-- /.row -->
            </div><!-- /.page-content -->
        </div>
    </div><!--Main Content-->
</div><!--main-container-->